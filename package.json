{
  "name": "synastria-launcher",
  "version": "1.1.0",
  "main": "main.js",
  "description": "Simple Electron launcher for Synastria WoW",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "package-win": "electron-packager . SynastriaLauncher --platform=win32 --arch=x64 --out=release-build --overwrite",
    "publish": "electron-builder --publish always"
  },
  "build": {
  "appId": "com.synastria.synastrialauncher",
  "publish": [
    {
      "provider": "github",
      "owner": "danhicks853",
      "repo": "SynLauncher"
    }
  ]
},
  "dependencies": {
    "adm-zip": "^0.5.16",
    "axios": "^1.9.0",
    "cheerio": "^1.0.0",
    "electron-updater": "^6.6.2",
    "extract-zip": "^2.0.1",
    "webtorrent": "^1.9.7",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "electron": "^25.3.1",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2"
  }
}
